			//左侧手风琴
		$('.sidebar>li').click(function() {
			$('.sidebar>li').css({
				'background': '#306bb5 url(/DataShare/img/img-02.png) no-repeat 195px  -405px'
			})
			if($(this).next().attr('id') == 'wei') {
				$('.sidebar').find('ul').attr('id', '').stop(true).slideUp();
				$(this).css({
					'background': '#306bb5 url(/DataShare/img/img-02.png) no-repeat 195px  -405px'
				})
			} else {
				$(this).css({
					'background': '#306bb5 url(/DataShare/img/img-02.png) no-repeat 195px  -437px'
				})
				$('.sidebar').find('ul').stop(true).slideUp();
				$(this).next().attr('id', 'wei').stop(true, true).slideDown();
			}
		})
	}())
