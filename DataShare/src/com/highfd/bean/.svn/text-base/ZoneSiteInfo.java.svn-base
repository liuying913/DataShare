package com.highfd.bean;

import java.util.List;

public class ZoneSiteInfo {
	
	private String id;
	private String zoneName;
	private List<SiteInfo> Citys;
	
	public ZoneSiteInfo() {
		super();
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}


	public List<SiteInfo> getCitys() {
		return Citys;
	}

	public void setCitys(List<SiteInfo> citys) {
		Citys = citys;
	}

	public String getZoneName() {
		return zoneName;
	}

	public void setZoneName(String zoneName) {
		this.zoneName = zoneName;
	}

	
	

}
